<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <objectAnimator
            android:propertyName="backgroundColor"
            android:duration="500"
            android:valueFrom="@color/naranjita"
            android:valueTo="@color/naranjita_oscuro" />
    </item>
    <item>
        <objectAnimator
            android:propertyName="backgroundColor"
            android:duration="500"
            android:valueFrom="@color/naranjita_oscuro"
            android:valueTo="@color/naranjita" />
    </item>
</selector>
